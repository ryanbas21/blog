		"next-redux-wrapper": "^1.3.5",
		"ramda": "^0.25.0",
		"react": "^16.3.1",
		"react-dom": "^16.3.1",
		"react-redux": "^5.0.7",
		"redux": "^3.7.2",
		"redux-observable": "1.0.0-alpha.2",
		"reselect": "^3.0.1",
		"rxjs": "^6.1.0",
		"semantic-ui-css": "^2.3.1",
		"semantic-ui-react": "^0.79.1"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.0.0-beta.46",

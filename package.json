{
	"name": "blog",
	"version": "1.0.0",
	"description": "A blog",
	"main": "index.js",
	"lint-staged": {
		"*.{ts, tsx,js,json,css,md}": ["prettier --write", "git add"]
	},
	"scripts": {
		"clean": "del .next",
		"precommit": "lint-staged",
		"pretest": "npm run lint",
		"lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
		"test": "./node_modules/.bin/jest --no-cache",
		"test:debug": "./node_modules/.bin/jest --debug",
		"test:verbose": "./node_modules/.bin/jest --verbose",
		"test:watch": "./node_modules/.bin/jest --watch",
		"test:coverage": "npm test -- --coverage --no-cache",
		"test:coverage:view":
			"npm test -- --coverage --no-cache && open coverage/lcov-report/index.html",
		"build": "npm run clean && next build",
		"dev": "npm run build && cross-env=development next"
	},
	"author": "Ryan Bas",
	"license": "MIT",
	"dependencies": {
		"buttercms": "^1.1.0",
		"material-ui": "^0.20.0",
		"moment": "^2.21.0",
		"next": "^5.0.1-canary.13",
		"next-redux-wrapper": "^1.3.5",
		"ramda": "^0.25.0",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-redux": "^5.0.7",
		"redux": "^3.7.2",
		"redux-observable": "^0.18.0",
		"reselect": "^3.0.1",
		"rxjs": "^5.5.6"
	},
	"devDependencies": {
		"@types/enzyme": "^3.1.9",
		"@types/enzyme-adapter-react-16": "^1.0.2",
		"@types/jest": "^22.1.4",
		"@types/material-ui": "^0.21.1",
		"@types/moment": "^2.13.0",
		"@types/next": "^2.4.8",
		"@types/next-redux-wrapper": "^1.3.7",
		"@types/node": "^9.4.6",
		"@types/ramda": "^0.25.19",
		"@types/react": "^16.0.40",
		"@types/react-addons-linked-state-mixin": "^0.14.19",
		"@types/react-dom": "^16.0.4",
		"@types/react-redux": "^5.0.15",
		"@types/reselect": "^2.2.0",
		"@zeit/next-css": "^0.1.3",
		"@zeit/next-typescript": "0.0.10",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"husky": "^0.15.0-rc.8",
		"jest": "^22.4.2",
		"lint-staged": "^7.0.0",
		"nextjs-export-path-map": "^0.1.2",
		"prettier": "1.11.1",
		"react-addons-test-utils": "^15.6.2",
		"redux-devtools-extension": "^2.13.2",
		"ts-jest": "^22.4.1",
		"ts-node": "^5.0.1",
		"tslint": "^5.9.1",
		"typescript": "^2.7.2",
		"typescript-babel-jest": "^1.0.5",
		"webpack-bundle-analyzer": "^2.10.0"
	}
}

{
	"name": "create-next-example-app",
	"lint-staged": {
		"*.{js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"scripts": {
		"dev": "nodemon server/index.js",
		"build": "next build",
		"start": "NODE_ENV=production next start",
		"precommit": "lint-staged",
		"pretest": "eslint .",
		"test:unit": "tape -r babel-register src/**/*.test.js",
		"test:view": "cls; tape -r babel-register src/**/*.test.jsx",
		"test": "npm run test:unit && npm run test:view"
	},
